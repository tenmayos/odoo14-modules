<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Must add this file to the manifest data array -->
    <data noupdate="1">
        <!-- Sequences for hospital.patient -->
        <!-- code just has to be something unique -->
        <!-- padding = how many digits -->
        <record id="hospital_patient_sequence" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>